{
    "name": "fb",
    "api_root": "https://graph.facebook.com",
    "resources": [
        {
            "path": "oauth/access_token",
            "methods": [ "get" ],
            "params": {
                "client_id": "required",
                "client_secret": "required",
                "grant_type": "required"
            }
        },
        {
            "path": "search",
            "methods": [ "get" ],
            "params": {
                "q": "optional",
                "access_token": "required"
            }
        },
        {
            "path": "me",
            "methods": [ "get" ],
            "params": {
                "fields": "optional",
                "access_token": "required"
            }
        }
        
    ]
}