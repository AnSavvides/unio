{
    "name": "fb",
    "api_root": "https://graph.facebook.com",
    "resources": {
        "search": {
            "path": "search",
            "methods": [ "get" ],
            "params": [
                {
                    "q": "optional"
                },
                {
                    "access_token": "required"
                }
            ]
        },
        "me": {
            "path": "me",
            "methods": [ "get" ],
            "params": [
                {
                    "fields": "optional"
                },
                {
                    "access_token": "required"
                }
            ]
        }
    }
}